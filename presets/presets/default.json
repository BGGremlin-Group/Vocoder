{
  "preset_name": "Default BGGG Vocoder Preset",
  "version": "5.0",
  "description": "Baseline settings for BGGG Vocoder V5.0 with balanced parameters, ParrotOS neon visualization, and voice cloning enabled.",
  "author": "BGGremlin Group",
  "date_created": "2025-07-12",

  "vocoder_settings": {
    "algorithm": "phase",
    "pitch_shift": 0,
    "modulation_index": 0.2,
    "modulation_rate": 1.5,
    "volume": 1.0
  },

  "visualization_settings": {
    "visualization_mode": "3D Particle Swarm",
    "color_theme": "ParrotOS Neon",
    "enable_interactive_controls": true
  },

  "voice_cloning_settings": {
    "enabled": true,
    "voice_model_path": "voice_models/trained_voice_model.pt",
    "speaker_embedding": "default_speaker",
    "prosody_transfer": true,
    "style_transfer": true,
    "real_time_adaptation": true
  },

  "audio_processing": {
    "noise_reduction": true,
    "vad_aggressiveness": 2,
    "latency_monitoring": true,
    "dynamic_chunk_adjustment": true
  },

  "ui_preferences": {
    "theme": "ParrotOS",
    "splash_screen": "assets/logo_screen.png",
    "exit_screen": "assets/exit_screen.png",
    "credits_screen": "assets/credits_screen.png",
    "static_visualizer": "assets/static_visualizer.png"
  }
}
